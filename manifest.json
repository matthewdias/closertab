{
	"manifest_version": 2,

	"name": "Closer Tab",
	"description": "Open tabs next to the current tab.",
	"version": "1.0",

	"permissions": ["contextMenus"],

	"background": {
		"scripts": ["background.js"]
	},
	"commands": {
		"newtab": {
			"suggested_key": {
				"default": "Alt+T"
			},
			"description": "Open new Closer Tab"
		}
	}
}
